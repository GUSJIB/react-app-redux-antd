{
  "name": "prompt-dee",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@reduxjs/toolkit": "^1.8.1",
    "antd": "^4.20.5",
    "axios": "^0.27.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-redux-loading-bar": "^5.0.4",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.1",
    "reactstrap": "^9.0.2",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
    "start": "npm run webapp:dev --",
    "build": "npm run clean && npm run webapp:build:prod --",
    "eject": "react-scripts eject",
    "pretest": "npm run lint",
    "test": "npm run jest --",
    "test-ci": "npm run lint && npm run jest:update --",
    "test:watch": "npm run jest -- --watch",
    "webapp:build": "npm run clean && npm run webapp:build:dev --",
    "webapp:build:dev": "webpack --mode=development --env stats=minimal",
    "webapp:build:prod": "webpack --mode=production --progress=profile",
    "webapp:dev": "npm run webpack-dev-server -- --mode=development --env stats=minimal",
    "webapp:dev-verbose": "npm run webpack-dev-server -- --mode=development --progress=profile --env stats=normal",
    "webapp:prod": "npm run clean && npm run webapp:build:prod --",
    "webapp:test": "npm run test --",
    "webpack-dev-server": "webpack serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-webpack-plugin": "3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "6.2.1",
    "prettier": "2.5.0",
    "prettier-plugin-packagejson": "2.2.15",
    "sass": "1.44.0",
    "sass-loader": "12.3.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "thread-loader": "3.0.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
